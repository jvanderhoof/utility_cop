<p id="notice"><%= notice %></p>

<h1>Listing App Environment Credentials</h1>

<table>
  <thead>
    <tr>
      <th>Credential</th>
      <th>App environment</th>
      <th>Encrypted value</th>
      <th>Text value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_environment_credentials.each do |app_environment_credential| %>
      <tr>
        <td><%= app_environment_credential.credential_id %></td>
        <td><%= app_environment_credential.app_environment_id %></td>
        <td><%= app_environment_credential.encrypted_value %></td>
        <td><%= app_environment_credential.text_value %></td>
        <td><%= link_to 'Show', app_environment_credential %></td>
        <td><%= link_to 'Edit', edit_app_environment_credential_path(app_environment_credential) %></td>
        <td><%= link_to 'Destroy', app_environment_credential, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App environment credential', new_app_environment_credential_path %>
